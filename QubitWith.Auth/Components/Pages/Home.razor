@page "/"
@using QubitWith.Auth.Data.Models.Entities.Statistics
@using QubitWith.Auth.Data.Persistence.Contracts

@inject IStatisticsRepository statisticRepository
@attribute [Authorize]
<PageTitle>Home</PageTitle>
<AuthorizeView>
    <StatisticsSectionAdmin  StatisticsAdmin="@statisticAdmin" />
</AuthorizeView>
@code {
    private StatisticsAdmin? statisticAdmin= new();
    protected override async Task OnInitializedAsync()
    {
        statisticAdmin = await statisticRepository.GetAdminStatistics();
    }
}

